ul.top-news li.sortable-dragging{border:3px solid #ccc;}
ul.top-news li:nth-of-type(1):before{content:'1';}
ul.top-news li:nth-of-type(2):before{content:'2';}
ul.top-news li:nth-of-type(3):before{content:'3';}
ul.top-news li:nth-of-type(4):before{content:'4';}
ul.top-news li:nth-of-type(5):before{content:'5';}
ul.top-news li.sortable-placeholder:before,ul.top-news li.sortable-dragging:before{content:'';}
ul.top-news li{-webkit-user-select:none;width:260px;float:left;box-sizing: border-box;margin:10px;padding:10px 10px 20px 10px;background:#eee; height:240px;overflow:hidden;text-overflow: ellipsis;border-radius:8px;}
.cms_float_bg{width:100%;position:fixed;top:0;left:100px;z-index: 999;}
.cms_float_main{display:none;width:700px;margin:100px auto;background: #fff;padding:40px;border-radius: 6px;box-shadow:0 40px 50px 30px rgba(0,0,0,.5);}
.cms_float_abstract{display:none;width:900px;margin:100px auto;background: #fff;padding:20px;border-radius:6px;box-shadow:0 40px 50px 30px rgba(0,0,0,.5);}
.cms_float_abstract .abstract-content{overflow-x:hidden;overflow-y: auto;}
.blur{-webkit-filter:blur(3px);}
.align-center{text-align: center;}
.cms_float_bg input[type=radio]{-webkit-appearance: radio;}
.cms_float_bg input[type=text]{
    border: 1px solid #CDCDCD;
    height: 26px;
    line-height: 26px;
    padding: 2px;
    border-radius: 2px;
    width: 500px;
    font-size: 20px;}
.cms_float_bg textarea{
    border: 1px solid #CDCDCD;
    height: 100px;
    line-height: 26px;
    padding: 2px;
    border-radius: 2px;
    width: 505px;
    max-width: 505px;
    font-size: 20px;
}

.add-weibo-card{
    position:absolute;
    z-index: 99999;
    top:0px;left:240px;
    background: #fff;
    border-radius: 10px;
    border:3px dashed #ccc;
    padding:40px;
    box-shadow: 0 20px 30px 10px rgba(0,0,0,0.7);
}
.add-weibo-card textarea{
    border: 1px solid #CDCDCD;
    height: 300px;
    min-height: 300px;
    max-width: 300px;
    line-height: 20px;
    padding: 2px;
    border-radius: 2px;
    width: 300px;
}
